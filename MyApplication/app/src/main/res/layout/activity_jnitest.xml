<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_jnitest"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="testjni.JNITestActivity">

    <Button
        android:id="@+id/bt_jni_test1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="测试无参jni接口" />

    <Button
        android:id="@+id/bt_jni_test2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="测试int类型参数接口" />

    <Button
        android:id="@+id/bt_jni_test3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="测试bool类型参数接口" />

    <Button
        android:id="@+id/bt_jni_test4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="测试String类型参数接口" />


    <Button
        android:id="@+id/bt_jni_test5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="测试数组类型参数接口" />

    <Button
        android:id="@+id/bt_jni_test6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="测试tcp连接" />

</LinearLayout>
